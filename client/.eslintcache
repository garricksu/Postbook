 new Date().getFullYear()\r\n    for (let i = currentYear; i >= 1905; i--) {\r\n      years.push(i)\r\n    }\r\n    return years.map((year, index) => (\r\n      <option key={index} value={year}>\r\n        {year}\r\n      </option>\r\n    ))\r\n  }\r\n\r\n  const updateInput = (e) => {\r\n    setInput({ ...input, [e.target.name]: e.target.value })\r\n  }\r\n\r\n  const onSubmitForm = async (e) => {\r\n    e.preventDefault()\r\n    if (checkInputDate()) {\r\n      const body = {\r\n        first_name: firstName,\r\n        last_name: lastName,\r\n        email,\r\n        password,\r\n        birthday: `${year} ${month} ${day}`,\r\n      }\r\n      registerUser(body)\r\n    }\r\n    try {\r\n    } catch (err) {\r\n      console.error(err.message)\r\n    }\r\n  }\r\n\r\n  // Check user birthday is valid date\r\n  const checkInputDate = () => {\r\n    const validDate = new Date(`${day} ${month} ${year}`)\r\n    const inputMonth = new Date(`1 ${month} ${year}`).getMonth()\r\n    console.log(input)\r\n    if (isNaN(validDate) || inputMonth !== validDate.getMonth()) {\r\n      setError(date)\r\n      return false\r\n    } else return true\r\n  }\r\n\r\n  return (\r\n    <Fragment>\r\n      {!error ? <div className=\"my-5 alert-size\"></div> : null}\r\n      <form\r\n        onSubmit={onSubmitForm}\r\n        className='my-3 mx-auto auth-form'\r\n        id='register-form'\r\n      >\r\n        <h2 className='text-center'>Register</h2>\r\n        <div className='form-group row'>\r\n          <div className='col'>\r\n            <input\r\n              onChange={(e) => updateInput(e)}\r\n              type='text'\r\n              name='firstName'\r\n              id='first-name'\r\n              placeholder='First Name'\r\n              className='form-control mt-3'\r\n              required\r\n            />\r\n          </div>\r\n          <div className='col'>\r\n            <input\r\n              onChange={(e) => updateInput(e)}\r\n              type='text'\r\n              name='lastName'\r\n              id='last-name'\r\n              placeholder='Last Name'\r\n              className='form-control mt-3'\r\n              required\r\n            />\r\n          </div>\r\n        </div>\r\n        <input\r\n          onChange={(e) => updateInput(e)}\r\n          type='email'\r\n          name='email'\r\n          id='email'\r\n          placeholder='Email'\r\n          className='form-control my-3'\r\n          required\r\n        />\r\n        <input\r\n          onChange={(e) => updateInput(e)}\r\n          type='password'\r\n          name='password'\r\n          id='password'\r\n          placeholder='Password'\r\n          className='form-control my-3'\r\n          required\r\n        />\r\n        <div className='form-group row'>\r\n          <div className='col'>\r\n            <label htmlFor='Month' className='col-form-label'>\r\n              <select\r\n                onChange={(e) => updateInput(e)}\r\n                name='month'\r\n                id='month'\r\n                className='form-control'\r\n                required\r\n              >\r\n                <option disabled default value=''>Month</option>\r\n                <option value='Jan'>Jan</option>\r\n                <option value='Feb'>Feb</option>\r\n                <option value='Mar'>Mar</option>\r\n                <option value='Apr'>Apr</option>\r\n                <option value='May'>May</option>\r\n                <option value='Jun'>Jun</option>\r\n                <option value='Jul'>Jul</option>\r\n                <option value='Aug'>Aug</option>\r\n                <option value='Sep'>Sep</option>\r\n                <option value='Oct'>Oct</option>\r\n                <option value='Nov'>Nov</option>\r\n                <option value='Dec'>Dec</option>\r\n              </select>\r\n            </label>\r\n          </div>\r\n          <div className='col d-flex justify-content-center'>\r\n            <label htmlFor='Month' className='col-form-label'>\r\n              <select\r\n                onChange={(e) => updateInput(e)}\r\n                name='day'\r\n                id='day'\r\n                className='form-control'\r\n              >\r\n                <option disabled default value=''>\r\n                  Day\r\n                </option>\r\n                {getDays()}\r\n              </select>\r\n            </label>\r\n          </div>\r\n          <div className='col d-flex justify-content-end'>\r\n            <label htmlFor='Month' className='col-form-label'>\r\n              <select\r\n                onChange={(e) => updateInput(e)}\r\n                name='year'\r\n                id='year'\r\n                className='form-control'\r\n                required\r\n              >\r\n                <option disabled default value=''>\r\n                  Year\r\n                </option>\r\n                {getYears()}\r\n              </select>\r\n            </label>\r\n          </div>\r\n        </div>\r\n        <button className='btn btn-primary btn-block'>Submit</button>\r\n        <Link to='/login' onClick={clearError}>\r\n          Login\r\n        </Link>\r\n      </form>\r\n    </Fragment>\r\n  )\r\n}\r\n\r\nexport default Register\r\n","C:\\Users\\Garrick\\Projects\\postbook\\client\\src\\components\\pages\\Dashboard.js",["106","107"],"import React, { Fragment, useEffect, useContext } from 'react'\r\n\r\nimport AuthContext from '../../context/auth/AuthContext'\r\nimport NavContext from '../../context/nav/NavContext'\r\n\r\nconst Dashboard = () => {\r\n  const authContext = useContext(AuthContext)\r\n\r\n  const {\r\n    getLoggedInUser,\r\n    clearLoggedInUser,\r\n    loggedInUser: { firstName, lastName, id },\r\n  } = authContext\r\n\r\n  const navContext = useContext(NavContext)\r\n  const { setActiveLink } = navContext\r\n\r\n  useEffect(() => {\r\n    getLoggedInUser()\r\n    setActiveLink(0)\r\n  }, [])\r\n\r\n  const logout = (e) => {\r\n    e.preventDefault()\r\n  