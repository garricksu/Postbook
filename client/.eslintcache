dex) => {\r\n    setActiveLink(index)\r\n    clearError()\r\n  }\r\n\r\n  return (\r\n    <nav className='navbar navbar-expand-lg navbar-dark bg-dark'>\r\n      <Link\r\n        className='navbar-brand'\r\n        id='brand-link'\r\n        to='/'\r\n        onClick={clearError}\r\n      >\r\n        Postbook\r\n      </Link>\r\n      {!isAuthenticated && !isLoading ? (\r\n        <ul className='navbar-nav ml-auto' id='nav-list'>\r\n          {generalLinks.map((link, index) => (\r\n            <li className='nav-item' key={index}>\r\n              <Link\r\n                className={\r\n                  index === active\r\n                    ? 'active active-underline nav-link'\r\n                    : 'nav-link'\r\n                }\r\n                id={link.id}\r\n                to={link.route}\r\n                onClick={() => changeActiveLink(index)}\r\n              >\r\n                {link.name}\r\n              </Link>\r\n            </li>\r\n          ))}\r\n        </ul>\r\n      ) : (\r\n        <ul className='navbar-nav ml-auto' id='nav-list'>\r\n          {userLinks.map((link, index) => (\r\n            <li className='nav-item' key={index}>\r\n              <Link\r\n                className={\r\n                  index === active\r\n                    ? 'active active-underline nav-link'\r\n                    : 'nav-link'\r\n                }\r\n                id={link.id}\r\n                to={link.route}\r\n                onClick={() => changeActiveLink(index)}\r\n              >\r\n                {link.name}\r\n              </Link>\r\n            </li>\r\n          ))}\r\n        </ul>\r\n      )}\r\n    </nav>\r\n  )\r\n}\r\n\r\nexport default Navbar\r\n","C:\\Users\\Garrick\\Projects\\postbook\\client\\src\\context\\nav\\NavState.js",[],"C:\\Users\\Garrick\\Projects\\postbook\\client\\src\\context\\nav\\NavContext.js",[],"C:\\Users\\Garrick\\Projects\\postbook\\client\\src\\context\\nav\\NavReducer.js",[],"C:\\Users\\Garrick\\Projects\\postbook\\client\\src\\context\\auth\\AuthContext.js",[],"C:\\Users\\Garrick\\Projects\\postbook\\client\\src\\context\\auth\\AuthState.js",[],"C:\\Users\\Garrick\\Projects\\postbook\\client\\src\\context\\auth\\AuthReducer.js",[],"C:\\Users\\Garrick\\Projects\\postbook\\client\\src\\context\\user\\UserState.js",[],"C:\\Users\\Garrick\\Projects\\postbook\\client\\src\\context\\user\\UserContext.js",[],"C:\\Users\\Garrick\\Projects\\postbook\\client\\src\\context\\user\\UserReducer.js",[],"C:\\Users\\Garrick\\Projects\\postbook\\client\\src\\components\\pages\\Profile.js",["131","132"],"import React, { useContext, useEffect, useState } from 'react'\r\nimport { withRouter } from 'react-router-dom'\r\n\r\nimport UserContext from '../../context/user/UserContext'\r\nimport AuthContext from '../../context/auth/AuthContext'\r\nimport PostContext from '../../context/post/PostContext'\r\n\r\nimport About from '../profile/About'\r\nimport CreatePost from '../post/CreatePost'\r\nimport Posts from '../post/Posts'\r\n\r\nconst Profile = (props) => {\r\n  const userContext = useContext(UserContext)\r\n  const {\r\n    getSelectedUser,\r\n    selectedUser: { id },\r\n    isLoading,\r\n  } = userContext\r\n\r\n  const authContext = useContext(AuthContext)\r\n  const { loggedInUser } = authContext\r\n\r\n  const postContext = useContext(PostContext)\r\n  const { getUserPosts } = postContext\r\n\r\n  useEffect(() => {\r\n    getSelectedUser(props.match.params.id)\r\n    getUserPosts(props.match.params.id)\r\n  }, [props.match.params.id])\r\n\r\n  const isValidUser = () => {\r\n    if (id === '') {\r\n      return (\r\n        <h1 className='text-center'>\r\n          Error 404<br></br>USER NOT FOUND\r\n        </h1>\r\n      )\r\n    } else {\r\n      return (\r\n        <div className='row'>\r\n          <About />\r\n          <div className='profile-feed col-6 mx-auto'>\r\n            {id === loggedInUser.id ? <CreatePost /> : null}\r\n            {!postContext.isLoading ? <Posts /> : null}\r\n          </div>\r\n\r\n          <div className='col'></div>\r\n        </div>\r\n      )\r\n    }\r\n  }\r\n\r\n  return !isLoading ? (\r\n    <div className='container my-5'>{isValidUser()}</div>\r\n  ) : null\r\n}\r\n\r\nexport default withRouter(Profile)\r\n","C:\\Users\\Garrick\\Projects\\postbook\\client\\src\\components\\pages\\Dashboard.js",["133","134"],"import React, { Fragment, useEffect, useContext } from 'react'\r\n\r\nimport AuthContext from '../../context/auth/AuthContext'\r\nimport NavContext from '../../context/nav/NavContext'\r\n\r\nconst Dashboard = () => {\r\n  const authContext = useContext(AuthContext)\r\n\r\n  const {\r\n    getLoggedInUser,\r\n    clearLoggedInUser,\r\n    loggedInUser: { firstName, lastName, id },\r\n  } = authContext\r\n\r\n  const navContext = useContext(NavContext)\r\n  const { setActiveLink } = navContext\r\n\r\n  useEffect(() => {\r\n    getLoggedInUser()\r\n    setActiveLink(0)\r\n  }, [])\r\n\r\n  const logout = (e) => {\r\n    e.preventDefault()\r\n    clearLoggedInUser()\r\n  }\r\n\r\n  return (\r\n    <Fragment>\r\n      <h1>Dashboard</h1>\r\n      <h2>\r\n        {firstName} {lastName}\r\n      </h2>\r\n      <button className='btn btn-primary' onClick={(e) => logout(e)}>\r\n        Logout\r\n      </button>\r\n    </Fragment>\r\n  )\r\n}\r\n\r\nexport default Dashboard\r\n","C:\\Users\\Garrick\\Projects\\postbook\\client\\src\\components\\pages\\Login.js",["135"],"C:\\Users\\Garrick\\Projects\\postbook\\client\\src\\components\\pages\\Register.js",["136"],"C:\\Users\\Garrick\\Projects\\postbook\\client\\src\\components\\pages\\Home.js",["137"],"import React, { useContext, useEffect } from 'react'\r\nimport NavContext from '../../context/nav/NavContext'\r\n\r\nconst Home = () => {\r\n  const navContext = useContext(NavContext)\r\n  const { setActiveLink } = navContext\r\n\r\n  useEffect(() => {\r\n    setActiveLink(0)\r\n  }, [])\r\n\r\n  return (\r\n    <div>\r\n      <h1 className='text-center'>Welcome To Postbook</h1>\r\n    </div>\r\n  )\r\n}\r\n\r\nexport default Home\r\n","C:\\Users\\Garrick\\Projects\\postbook\\client\\src\\components\\profile\\About.js",["138"],"C:\\Users\\Garrick\\Projects\\postbook\\client\\src\\context\\post\\PostState.js",["139","140","141"],"import React, { useReducer } from 'react'\r\n\r\nimport PostContext from './PostContext'\r\nimport PostReducer from './PostReducer'\r\nimport {\r\n  GET_USER_POSTS,\r\n  DELETE_POST,\r\n  GET_DASHBOARD_POSTS,\r\n  SET_LOADING,\r\n} from '../types'\r\n\r\nimport axios from 'axios'\r\n\r\nconst PostState = (props) => {\r\n  const initialState = {\r\n    posts: [],\r\n    isLoading: true,\r\n  }\r\n\r\n  const [state, dispatch] = useReducer(PostReducer, initialState)\r\n\r\n  // Creat new post\r\n  const submitPost = async (body) => {\r\n    const config = { headers: { 'Content-Type': 'application/json' } }\r\n    try {\r\n      const response = await axios.post(\r\n        `http://localhost:5000/api/posts/new/${body.id}`,\r\n        body,\r\n        config\r\n      )\r\n      getUserPosts(body.id)\r\n    } catch (err) {\r\n      console.log(err)\r\n    }\r\n  }\r\n\r\n  const getUserPosts = async (selectedUserId) => {\r\n    try {\r\n      const response = await axios.get(\r\n        `http://localhost:5000/api/posts/${selectedUserId}`\r\n      )\r\n      dispatch({\r\n        type: GET_USER_POSTS,\r\n        payload: response.data.userPosts,\r\n      })\r\n    } catch (err) {\r\n      console.log(err)\r\n    }\r\n  }\r\n\r\n  const setLoading = (loading) => {\r\n    dispatch({ type: SET_LOADING, payload: loading })\r\n  }\r\n\r\n  return (\r\n    <PostContext.Provider\r\n      value={{\r\n        posts: state.posts,\r\n        isLoading: state.isLoading,\r\n        getUserPosts,\r\n        submitPost,\r\n        setLoading,\r\n      }}\r\n    >\r\n      {props.children}\r\n    </PostContext.Provider>\r\n  )\r\n}\r\n\r\nexport default PostState\r\n","C:\\Users\\Garrick\\Projects\\postbook\\clien