ck\r\n","C:\\Users\\Garrick\\Projects\\postbook\\client\\src\\utils\\Alerts.js",[],"C:\\Users\\Garrick\\Projects\\postbook\\client\\src\\utils\\constants.js",[],"C:\\Users\\Garrick\\Projects\\postbook\\client\\src\\components\\layout\\Navbar.js",["126"],"import React, { useContext, useEffect } from 'react'\r\nimport { Link } from 'react-router-dom'\r\n\r\nimport AuthContext from '../../context/auth/AuthContext'\r\nimport NavContext from '../../context/nav/NavContext'\r\nimport UserContext from '../../context/user/UserContext'\r\n\r\nconst Navbar = () => {\r\n  const authContext = useContext(AuthContext)\r\n  const {\r\n    isAuthenticated,\r\n    clearError,\r\n    loggedInUser: { id },\r\n    getLoggedInUser,\r\n    isLoading,\r\n  } = authContext\r\n\r\n  const navContext = useContext(NavContext)\r\n  const { active, setActiveLink } = navContext\r\n\r\n  const userContext = useContext(UserContext)\r\n  const {selectedUser } = userContext\r\n\r\n  useEffect(() => {\r\n    if (isAuthenticated) {\r\n      getLoggedInUser()\r\n      setProfileIndex()\r\n    }\r\n  }, [isAuthenticated])\r\n\r\n  const setProfileIndex = () => {\r\n    if (id === selectedUser.id && !userContext.isLoading) {\r\n      setActiveLink(1)\r\n    }\r\n  }\r\n\r\n  const generalLinks = [\r\n    { name: 'Home', route: '/', id: 'home-link' },\r\n    { name: 'Login', route: '/login', id: 'login-link' },\r\n    { name: 'Register', route: '/register', id: 'register-link' },\r\n  ]\r\n  const userLinks = [\r\n    { name: 'Home', route: '/', id: 'home-link' },\r\n    { name: 'Profile', route: `/user/${id}`, id: 'profile-link' },\r\n    { name: 'Settings', route: '/', id: 'settings-link' },\r\n  ]\r\n\r\n  const changeActiveLink = (index) => {\r\n    setActiveLink(index)\r\n    clearError()\r\n  }\r\n\r\n  return (\r\n    <nav className='navbar navbar-expand-lg navbar-dark bg-dark'>\r\n      <Link\r\n        className='navbar-brand'\r\n        id='brand-link'\r\n        to='/'\r\n        onClick={clearError}\r\n      >\r\n        Postbook\r\n      </Link>\r\n      {!isAuthenticated && !isLoading ? (\r\n        <ul className='navbar-nav ml-auto' id='nav-list'>\r\n          {generalLinks.map((link, index) => (\r\n            <li className='nav-item' key={index}>\r\n              <Link\r\n                className={\r\n                  index === active\r\n                    ? 'active active-underline nav-link'\r\n                    : 'nav-link'\r\n                }\r\n                id={link.id}\r\n                to={link.route}\r\n                onClick={() => changeActiveLink(index)}\r\n              >\r\n                {link.name}\r\n              </Link>\r\n            </li>\r\n          ))}\r\n        </ul>\r\n      ) : (\r\n        <ul className='navbar-nav ml-auto' id='nav-list'>\r\n          {userLinks.map((link, index) => (\r\n            <li className='nav-item' key={index}>\r\n              <Link\r\n                className={\r\n                  index === active\r\n                    ? 'active active-underline nav-link'\r\n                    : 'nav-link'\r\n                }\r\n                id={link.id}\r\n                to={link.route}\r\n                onClick={() => changeActiveLink(index)}\r\n              >\r\n                {link.name}\r\n              </Link>\r\n            </li>\r\n          ))}\r\n        </ul>\r\n      )}\r\n    </nav>\r\n  )\r\n}\r\n\r\nexport default Navbar\r\n","C:\\Users\\Garrick\\Projects\\postbook\\client\\src\\context\\nav\\NavState.js",[],"C:\\Users\\Garrick\\Projects\\postbook\\client\\src\\context\\nav\\NavContext.js",[],"C:\\Users\\Garrick\\Projects\\postbook\\client\\src\\context\\nav\\NavReducer.js",[],"C:\\Users\\Garrick\\Projects\\postbook\\client\\src\\context\\auth\\AuthContext.js",[],"C:\\Users\\Garrick\\Projects\\postbook\\client\\src\\context\\auth\\AuthState.js",[],"C:\\Users\\Garrick\\Projects\\postbook\\client\\src\\context\\auth\\AuthReducer.js",[],"C:\\Users\\Garrick\\Projects\\postbook\\client\\src\\components\\pages\\Register.js",["127"],"import React, { Fragment, useState, useContext, useEffect } from 'react'\r\nimport { Link } from 'react-router-dom'\r\n\r\nimport AuthContext from '../../context/auth/AuthContext'\r\nimport NavContext from '../../context/nav/NavContext'\r\n\r\nimport * as Constants from '../../utils/constants'\r\n\r\nconst Register = (props) => {\r\n  const authContext = useContext(AuthContext)\r\n  const { isAuthenticated, registerUser, error, setError, clearError } = authContext\r\n\r\n  const navContext = useContext(NavContext)\r\n  const { setActiveLink } = navContext\r\n\r\n  const [input, setInput] = useState({\r\n    firstName: '',\r\n    lastName: '',\r\n    email: '',\r\n    password: '',\r\n    month: '',\r\n    day: '',\r\n    year: '',\r\n  })\r\n\r\n  const { firstName, lastName, email, password, month, day, year } = input\r\n\r\n  const { date } = Constants\r\n\r\n  useEffect(() => {\r\n    if (isAuthenticated) {\r\n      props.history.push('/')\r\n      setActiveLink(0)\r\n    } else {\r\n      setActiveLink(2)\r\n    }\r\n  }, [isAuthenticated, props.history])\r\n\r\n  // List of dates for select menu\r\n  const getDays = () => {\r\n    let days = []\r\n    for (let i = 1; i <= 31; i++) {\r\n      days.push(i)\r\n    }\r\n    return days.map((day, index) => (\r\n      <option key={index} value={day}>\r\n        {day}\r\n      </option>\r\n    ))\r\n  }\r\n\r\n  //  List of years for select menu\r\n  const getYears = () => {\r\n    let years = []\r\n    const currentYear = new Date().getFullYear()\r\n    for (let i = currentYear; i >= 1905; i--) {\r\n      years.push(i)\r\n    }\r\n    return years.map((year, index) => (\r\n      <option key={index} value={year}>\r\n        {year}\r\n      </option>\r\n    ))\r\n  }\r\n\r\n  const updateInput = (e) => {\r\n    setInput({ ...input, [e.target.name]: e.target.value })\r\n  }\r\n\r\n  const onSubmitForm = async (e) => {\r\n    e.preventDefault()\r\n    if (checkInputDate()) {\r\n      const body = {\r\n        first_name: firstName,\r\n        last_name: lastName,\r\n        email,\r\n        password,\r\n        birthday: `${year} ${month} ${day}`,\r\n      }\r\n      registerUser(body)\r\n    }\r\n    try {\r\n    } catch (err) {\r\n      console.error(err.message)\r\n    }\r\n  }\r\n\r\n  // Check user birthday is valid date\r\n  const checkInputDate = () => {\r\n    const validDate = new Date(`${day} ${month} ${year}`)\r\n    const inputMonth = new Date(`1 ${month} ${year}`).getMonth()\r\n    console.log(input)\r\n    if (isNaN(validDate) || inputMonth !== validDate.getMonth()) {\r\n      setError(date)\r\n      return false\r\n    } else return true\r\n  }\r\n\r\n  return (\r\n    <Fragment>\r\n      {!error ? <div className=\"my-5 alert-size\"></div> : null}\r\n      <form\r\n        onSubmit={onSubmitForm}\r\n        className='my-3 mx-auto auth-form'\r\n        id='register-form'\r\n      >\r\n        <h2 className='text-center'>Register</h2>\r\n        <div className='form-group row'>\r\n          <div className='col'>\r\n            <input\r\n              onChange={(e) => updateInput(e)}\r\n              type='text'\r\n              name='firstName'\r\n              id='first-name'\r\n              placeholder='First Name'\r\n              className='form-control mt-3'\r\n              required\r\n            />\r\n          </div>\r\n          <div className='col'>\r\n            <input\r\n              onChange={(e) => updateInput(e)}\r\n              type='text'\r\n              name='lastName'\r\n              id='last-name'\r\n              placeholder='Last Name'\r\n              className='form-control mt-3'\r\n              required\r\n            />\r\n          </div>\r\n        </div>\r\n        <input\r\n          onChange={(e) => updateInput(e)}\r\n          type='email'\r\n          name='email'\r\n          id='email'\r\n          placeholder='Email'\r\n          className='form-control my-3'\r\n          required\r\n        />\r\n        <input\r\n          onChange={(e) => updateInput(e)}\r\n          type='password'\r\n          name='password'\r\n          id='password'\r\n          placeholder='Password'\r\n          className='form-control my-3'\r\n          required\r\n        />\r\n        <div className='form-group row'>\r\n          <div className='col'>\r\n            <label htmlFor='Month' className='col-form-label'>\r\n              <select\r\n                onChange={(e) => updateInput(e)}\r\n                name='month'\r\n                id='month'\r\n                className='form-control'\r\n                required\r\n              >\r\n                <option disabled default value=''>Month</option>\r\n                <option value='Jan'>Jan</option>\r\n                <option value='Feb'>Feb</option>\r\n                <option value='Mar'>Mar</option>\r\n                <option value='Apr'>Apr</option>\r\n                <option value='May'>May</option>\r\n                <option value='Jun'>Jun</option>\r\n                <option value='Jul'>Jul</option>\r\n                <option value='Aug'>Aug</option>\r\n                <option value='Sep'>Sep</option>\r\n                <option value='Oct'>Oct</option>\r\n                <option value='Nov'>Nov</option>\r\n                <option value='Dec'>Dec</option>\r\n              </select>\r\n            </label>\r\n          </div>\r\n          <div className='col d-flex justify-content-center'>\r\n            <label htmlFor='Month' className='col-form-label'>\r\n              <select\r\n                onChange={(e) => updateInput(e)}\r\n                name='day'\r\n                id='day'\r\n                className='form-control'\r\n              >\r\n                <option disabled default value=''>\r\n                  Day\r\n                </option>\r\n                {getDays()}\r\n              </select>\r\n            </label>\r\n          </div>\r\n          <div className='col d